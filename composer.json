{
    "name": "php-ddd/domain-driven-design",
    "description": "Tools/framework to ease the implementation of Eric Evans Domain Driven Design in PHP.",
    "minimum-stability": "stable",
    "license": "MIT",
    "autoload": {
        "psr-4": {"PhpDDD\\DomainDrivenDesign\\": "src"}
    },
    "autoload-dev": {
        "psr-4": {"PhpDDD\\DomainDrivenDesign\\Tests\\": "tests"}
    },
    "authors": [
        {
            "name": "Julien Dufresne"
        }
    ],
    "require": {
        "php": ">=5.5",
        "php-ddd/utils": "^1.2.0"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "^1.8.1",
        "phpmd/phpmd": "^2.2.3",
        "phpunit/phpunit": "^4.7.0",
        "phploc/phploc": "^2.1.3",
        "satooshi/php-coveralls": "dev-master"
    }
}
